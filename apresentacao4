        <script>
            const json = {
                "clientes" :
            [
                {
                        "nome" : "João",
                        "idade" : 30
                    },
                    {
                        "nome" : "José",
                        "idade" : 35


                }
            
            ]    
        }
            
        json.clientes.forEach(elemento =>{
                console.log(elemento.nome);
                document.write('<tr>');
                document.write('<td>' + elemento.nome + '</td>');
                document.write('<td>'+ elemento.idade + '</td>');
                document.write('</tr>');
            });
        
        
        
        
        </script>
        </table>
